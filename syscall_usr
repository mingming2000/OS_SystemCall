# define syscall_push 335
# define syscall_pop 336

# include <unistd.h>
# include <stdlib.h>
# include <stdio.h>
#include <time.h>

int main(){
    int push = 0;
    int pop = 0;
    
    srand(time(NULL));

    // Push three times.
    for(int i = 0; i < 3; ++i){
        push = syscall(syscall_push, rand()%99 + 1);
        printf("Push : %3d\n", push);
    }


    // Pop three times.
    for(int i = 0; i < 3; ++i){
        pop = syscall(syscall_pop);
        printf("Pop  : %3d\n", pop);
    }

    return 0;
}